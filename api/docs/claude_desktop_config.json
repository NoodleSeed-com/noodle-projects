{
  "mcpServers": {
    "noodle-projects": {
      "command": "mcp",
      "args": ["dev", "[PATH_TO_PROJECT]/api/app/mcp_server_supabase.py"],
      "env": {
        "PYTHONPATH": "[PATH_TO_PROJECT]",
        "SUPABASE_URL": "your_supabase_project_url",
        "SUPABASE_KEY": "your_supabase_service_role_key"
      }
    }
  }
}